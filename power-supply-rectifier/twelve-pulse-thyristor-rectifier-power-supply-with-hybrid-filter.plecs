"w_g = (k_p*u_gq) + w_gi;\n"
"\n"
"w_gi_new = w_gi + (T_s*k_i*u_gq);\n"
"\n"
"theta_g_new = theta_g + (T_s*w_g);\n"
"\n"
"if(theta_g_new > 2*PI){\n"
"\ttheta_g_new = theta_g_new - 2*PI;\n"
"}else if(theta_g_new < 0){\n"
"\ttheta_g_new = theta_g_new + 2*PI;\n"
"}\n"
"\n"
"OutputSignal(0,0) = theta_g_new;"
