                Show          off
              }
              Parameter {
                Variable      "Ts"
                Value         "T_s"
                Show          off
              }
              Parameter {
                Variable      "Parameters"
                Value         "alpha_c, L_f, w_g, u_s"
                Show          off
              }
              Parameter {
                Variable      "LangStandard"
                Value         "2"
                Show          off
              }
              Parameter {
                Variable      "GnuExtensions"
                Value         "1"
                Show          off
              }
              Parameter {
                Variable      "RuntimeCheck"
                Value         "2"
                Show          off
              }
              Parameter {
                Variable      "Declarations"
                Value         "#import <math.h>\n"
"#import <complex.h>\n"
"\n"
"#define i_d_ref InputSignal(0,0)\n"
"#define i_q_ref InputSignal(0,1)\n"
"#define i_d InputSignal(0,2)\n"
"#define i_q InputSignal(0,3)\n"
"#define theta_g InputSignal(0,4)\n"
"#define u_dc InputSignal(0,5)\n"
"\n"
"#define PI 3.14159265358979323846\n"
"\n"
"static double alpha_c, L_f, w_g, k_i, k_t, T_s;\n"
"static complex double k_p, u_i;\n"
"\n"
"complex double i_ref, u_ref, u_s_ref, u_s_ref_lim, u_ref_lim, u_i_new, i;\n"
"double theta_gc, u_alpha_ref, u_beta_ref, u_a_ref, u_b_ref, u_c_ref, max_u_va"
"lue, min_u_value, u0, m, u_gN;\n"
"\n"
"int index;"
                Show          off
              }
              Parameter {
                Variable      "StartFcn"
                Value         "//System parameters\n"
"alpha_c = ParamRealData(0,0);\n"
"L_f = ParamRealData(1,0);\n"
"w_g = ParamRealData(2,0);\n"
"u_gN = ParamRealData(3,0);\n"
"\n"
"// Gains\n"
"k_i = pow(alpha_c, 2)*L_f;      // Integral gain\n"
"k_t = alpha_c*L_f;              // Reference gain\n"
"k_p = (2*alpha_c - I*w_g)*L_f;  // Proportional gain\n"
"\n"
"//Initial condition\n"
"u_i = u_gN;"
                Show          off
              }
              Parameter {
                Variable      "OutputFcn"
                Value         "// Calculating i from i_d and i_q\n"
"i = i_d + I*i_q;\n"
"\n"
"// Calculating i_ref from i_d_ref and i_q_ref\n"
"i_ref = i_d_ref + I*i_q_ref;\n"
"\n"
"// Voltage reference in synchronous coordinates\n"
"u_ref = k_t*i_ref - k_p*i + u_i;\n"
"\n"
"// Reference in stator coordinates\n"
"theta_gc = theta_g + 1.5*T_s*w_g;   // Compensation for discrete-time delays"
"\n"
"u_s_ref = u_ref*cexp(I*theta_gc);\n"
"\n"
"// Symmetrical suboscillation PWM\n"
"// Components of the voltage space vector\n"
"u_alpha_ref = creal(u_s_ref);\n"
"u_beta_ref  = cimag(u_s_ref);\n"
"\n"
"// Phase-voltage references\n"
"u_a_ref = u_alpha_ref;\n"
"u_b_ref = 0.5*(-u_alpha_ref + sqrt(3)*u_beta_ref);\n"
"u_c_ref = 0.5*(-u_alpha_ref - sqrt(3)*u_beta_ref);\n"
"\n"
"// Vector of phase-voltage references\n"
"double u_abc_ref[3] = {u_a_ref, u_b_ref, u_c_ref};\n"
"\n"
"// Finding maximum value\n"
"max_u_value = u_abc_ref[0];\n"
"for(index = 1; index < 3; ++index) {\n"
"\tif(u_abc_ref[index] > max_u_value){\n"
"      max_u_value = u_abc_ref[index];\n"
"  \t}\n"
"}\n"
"\n"
"//Finding minimum value\n"
"min_u_value = u_abc_ref[0];\n"
"for(index = 1; index < 3; ++index) {\n"
"\tif(u_abc_ref[index] < min_u_value){\n"
"      min_u_value = u_abc_ref[index];\n"
"  \t}\n"
"}\n"
"\n"
"// Symmetrization of the references by adding the zero-sequence voltage\n"
"u0 = 0.5*(max_u_value + min_u_value);\n"
"for(index=0; index<3 ; index++){\n"
"\tu_abc_ref[index] = u_abc_ref[index] - u0;\n"
"}\n"
"\n"
"// Clipping of the reference voltages such that no phase error is caused. \n"
"// If all phase voltage reference are realizable (below u_dc/2), m = 1 holds."
"\n"
"double u_abc_ref_for_m[4] = {0,0,0,1};\n"
"for(index=0; index<3; index++){\n"
"\tu_abc_ref_for_m[index] = (2*u_abc_ref[index])/u_dc;\n"
"}\n"
"\n"
"//Extracting m\n"
"m = u_abc_ref_for_m[0];\n"
"for(index = 1; index < 4; ++index) {\n"
"\tif(u_abc_ref[index] > max_u_value){\n"
"      m = u_abc_ref_for_m[index];\n"
"  \t}\n"
"}\n"
"\n"
"//Clipping of reference voltage\n"
"for(index=0; index<3 ; index++){\n"
"\tu_abc_ref[index] = u_abc_ref[index]/m;\n"
"}\n"
"\n"
"// Duty cycles, which can be directly used for triangle comparison, \n"
"// if the triangle wave varies between 0...1\n"
"double d_abc[3] = {0, 0, 0};\n"
"for(index=0; index<3 ; index++){\n"
"\td_abc[index] = 0.5 + u_abc_ref[index]/u_dc;\n"
"}\n"
"\n"
"// Realized voltage vector is calculated from the clamped duty ratios.\n"
"// If the PWM does not saturate, u_c_ref_lim = u_c_ref holds. The realized\n"
"// voltage vector is needed, e.g., for the current-controller antiwindup.\n"
"u_s_ref_lim = (2/3)*(d_abc[0] + d_abc[1]*cexp(I*2*PI/3) + d_abc[2]*cexp(I*4*P"
"I/3))*u_dc;\n"
"\n"
"// In estimated rotor flux coordinates\n"
"u_ref_lim = exp(-I*theta_gc)*u_s_ref_lim;\n"
"\n"
"// Update the integral state of the current controller \n"
"u_i_new = u_i + T_s*k_i*(i_ref - i + (u_ref_lim - u_ref)/k_t);\n"
"\n"
"OutputSignal(0,0) = creal(u_s_ref);\n"
"OutputSignal(0,1) = cimag(u_s_ref);"
                Show          off
