                Value         "#import <math.h>\n"
"#import <complex.h>\n"
"\n"
"#define u_dc_ref InputSignal(0,0)\n"
"#define u_dc InputSignal(0,1)\n"
"\n"
"static double p_gi, C, zeta, w0_dc, p_max, T_s;\n"
"\n"
"double e, k_p, k_i, p_g_ref, p_g_ref_lim, p_gi_new;"
                Show          off
              }
              Parameter {
                Variable      "StartFcn"
                Value         "// System parameters\n"
"C = ParamRealData(0,0);\n"
"zeta = ParamRealData(1,0);\n"
"w0_dc = ParamRealData(2,0);\n"
"p_max = ParamRealData(3,0);\n"
"T_s = ParamRealData(4,0);\n"
"\n"
"// Gains\n"
"k_p = 2*zeta*w0_dc;\n"
"k_i = pow(w0_dc, 2);\n"
"\n"
"// Initial integral condition\n"
"p_gi = 0;"
                Show          off
              }
              Parameter {
                Variable      "OutputFcn"
                Value         "//Error signal\n"
"e = (C/2)*(pow(u_dc_ref, 2) - pow(u_dc, 2));\n"
"\n"
"p_g_ref = (-k_p)*e - p_gi;\n"
"\n"
"p_g_ref_lim = p_g_ref;\n"
"\n"
"if(p_g_ref_lim > p_max){\n"
"\tp_g_ref_lim = p_max;\n"
"}\n"
"else if (p_g_ref_lim < -p_max){\n"
"\tp_g_ref_lim = -p_max;\n"
"}\n"
"\n"
"// Update the integrator state (the last term is antiwindup)\n"
"p_gi_new = p_gi + T_s*k_i *(e + (p_g_ref_lim - p_g_ref)/k_p);\n"
"\n"
"// Output signal\n"
"OutputSignal(0,0) = p_g_ref_lim;"
                Show          off
              }
              Parameter {
                Variable      "UpdateFcn"
                Value         "p_gi = p_gi_new;"
                Show          off
